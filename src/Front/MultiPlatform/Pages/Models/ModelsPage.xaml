<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MultiPlatform.Pages.Models.ModelsPage">
    <StackLayout>
        <ActivityIndicator x:Name="activityIndicator" IsRunning="True" IsVisible="True" />

        <ListView x:Name="listView" ItemSelected="OnItemSelected" IsVisible="False">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <TextCell Text="{Binding Id}" />
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
    </StackLayout>
</ContentPage>
